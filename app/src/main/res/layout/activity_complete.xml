<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".complete.completeActivity"
    android:gravity="center"
    android:background="#a890fe">

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="350dp"
        android:orientation="vertical"
        android:gravity="center"
        android:background="@drawable/blue_radius"
        >
        <ImageView
            android:layout_width="68dp"
            android:layout_height="148dp"
            android:background="@drawable/handmoney">

        </ImageView>

    <TextView
        android:id="@+id/completeTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:fontFamily="@font/blackhansans_regular"
        android:textStyle="normal"
        android:layout_marginTop="50dp"
        android:textSize="25dp"
        android:text="인증이 완료되었습니다."></TextView>

    <Button
        android:id="@+id/btnCheck"
        android:layout_width="210dp"
        android:layout_height="45dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/button_round"
        android:textSize="15dp"
        android:layout_gravity="center"
        android:fontFamily="@font/notoxanscjkkr_bold"
        android:textStyle="normal"
        android:text="확인"
        android:textColor="#ffffff"/>

    </LinearLayout>



</LinearLayout>